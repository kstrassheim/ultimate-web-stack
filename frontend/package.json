{
  "name": "ultimate-web-stack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:e2e": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 cypress\"",
    "test:e2e:chrome": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 cypress:chrome\"",
    "test:e2e:edge": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 cypress:edge\"",
    "test:e2e:headless": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 'cypress run --browser chrome'\"",
    "test": "jest",
    "test:preview": "npx jest-preview",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "node start-backend.js",
    "backend:test": "cross-env MOCK=true node start-backend.js",
    "backend:check": "start-server-and-test backend http://127.0.0.1:8000/health",
    "cypress": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open --e2e --browser chrome",
    "cypress:chrome": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --headed --browser chrome --e2e",
    "cypress:edge": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --headed --browser edge --e2e",
    "test:backend": "cross-env MOCK=true node start-backend.js --host 0.0.0.0",
    "test:frontend": "cross-env npm_config_mock=true vite --port 5173 --host"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.7.0",
    "@azure/msal-react": "^3.0.6",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "bootstrap": "^5.3.5",
    "notyf": "^3.10.0",
    "react": "^18.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@swc/core": "^1.11.13",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preview": "^0.2.3",
    "start-server-and-test": "^2.0.11",
    "vite": "^6.2.0"
  }
}
