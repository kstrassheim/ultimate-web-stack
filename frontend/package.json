{
  "name": "ultimate-web-stack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:e2e": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 cypress\"",
    "test:e2e:run": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 'cypress run --browser edge --headed'\"",
    "test:e2e:headless": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 'cypress run --browser edge'\"",
    "test:e2e:coverage": "start-server-and-test test:backend http://0.0.0.0:8000/health \"start-server-and-test test:frontend http://localhost:5173 'cypress run --browser edge --env coverage=true'\"",
    "check-coverage": "nyc check-coverage",
    "coverage:report:no-check": "nyc report --reporter=lcov --reporter=html --reporter=text-summary --check-coverage=false",
    "test": "jest",
    "test:preview": "npx jest-preview",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "node start-backend.js",
    "backend:test": "cross-env MOCK=true node start-backend.js",
    "backend:check": "start-server-and-test backend http://127.0.0.1:8000/health",
    "cypress": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open --e2e --browser chrome",
    "cypress:edge": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --headed --browser edge --e2e",
    "test:backend": "cross-env MOCK=true LOG_LEVEL=ERROR node start-backend.js --host 0.0.0.0",
    "test:frontend": "cross-env npm_config_mock=true vite --port 5173 --host",
    "coverage:e2e-only": "npm run test:e2e:coverage && npm run coverage:report:e2e",
    "coverage:report:e2e": "nyc report --reporter=lcov --reporter=html --reporter=text-summary",
    "coverage:merge": "nyc merge .nyc_output coverage/merged-coverage.json",
    "coverage:report": "nyc report --reporter=lcov --reporter=html --reporter=text-summary",
    "coverage:all": "npm run test:coverage && npm run test:e2e:coverage && npm run coverage:merge && npm run coverage:report"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.7.0",
    "@azure/msal-react": "^3.0.6",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "apexcharts": "^4.5.0",
    "bootstrap": "^5.3.5",
    "notyf": "^3.10.0",
    "react": "^18.0.0",
    "react-apexcharts": "^1.7.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/js": "^9.21.0",
    "@swc/core": "^1.11.13",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.1",
    "cypress-wait-until": "^3.0.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preview": "^0.2.3",
    "nyc": "^17.1.0",
    "start-server-and-test": "^2.0.11",
    "vite": "^6.2.0",
    "vite-plugin-istanbul": "^7.0.0"
  }
}
